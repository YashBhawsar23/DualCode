{
	"compilerOptions": {
	  "target": "ES6",
	  "module": "CommonJS",
	  "outDir": "./dist",
	  "rootDir": "./src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "resolveJsonModule": true,
	  "types": ["node"]
	}
  }

