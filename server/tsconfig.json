{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "CommonJS",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "outDir": "./dist",
	  "types": ["node"], 
	  "moduleResolution": "node", 
	  "resolveJsonModule": true 
	},
	"include": ["src"],
	"exclude": ["node_modules"]
  }

